{
  "name": "notes-app",
  "version": "1.0.0",
  "description": "Notes app implementation inspired by Google Keep developed with NextJS and Prisma.",
  "author": "Jan Bauer",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p $PORT",
    "lint": "next lint",
    "test": "jest",
    "export": "export"
  },
  "dependencies": {
    "@fontsource/roboto": "^5.0.14",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@material-ui/styles": "^4.11.5",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.18.0",
    "@reduxjs/toolkit": "^2.2.7",
    "@svgr/webpack": "^8.1.0",
    "@types/material-ui": "^0.21.17",
    "@types/react-redux": "^7.1.33",
    "@types/styled-components": "^5.1.34",
    "babel-plugin-styled-components": "^2.1.4",
    "bootstrap": "^5.3.3",
    "framer-motion": "^11.3.21",
    "next": "14.2.5",
    "next-auth": "^4.24.7",
    "react": "18.3.1",
    "react-color": "^2.19.3",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.52.2",
    "react-redux": "^9.1.2",
    "react-toastify": "^10.0.5",
    "redux-saga": "^1.3.0",
    "sass": "^1.77.8",
    "sqlite3": "^5.1.7",
    "styled-components": "^6.1.12",
    "swr": "^2.2.5"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.18",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.1.0",
    "@types/react": "18.3.3",
    "@types/react-color": "^3.0.12",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.8.0",
    "autoprefixer": "^10.4.20",
    "enzyme": "^3.11.0",
    "eslint": "9.8.0",
    "eslint-config-next": "14.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "postcss": "^8.4.41",
    "prisma": "^5.18.0",
    "ts-jest": "^29.2.4",
    "typescript": "5.5.4"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss|sass|less)$": "<rootDir>/style-mock.js",
      ".+\\.(svg|png|jpg)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "testPathIgnorePatterns": [
      "./.next/",
      "./node_modules/"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.jest.json"
      }
    }
  },
  "resolutions": {
    "styled-components": "^5"
  }
}
